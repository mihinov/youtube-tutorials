{
  "name": "youtube-tutorials",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build:gsap-circle": "cross-env PROJECT=youtube-gsap-circle vite build --emptyOutDir",
    "build:perspective-card": "cross-env PROJECT=youtube-perspective-card vite build --emptyOutDir",
    "build:angular-tree": "cd ./youtube-angular-tree && npm run build:github",
    "build:security-supplies": "cross-env PROJECT=youtube-security-supplies vite build --emptyOutDir",
    "build:pointer-events": "cross-env PROJECT=pointer-events vite build --emptyOutDir",
    "build:youtube-experience-bar": "cross-env PROJECT=youtube-experience-bar vite build --emptyOutDir",
    "build:youtube-game-of-life-ts": "cross-env PROJECT=youtube-game-of-life/only-ts vite build --emptyOutDir",
    "build:youtube-game-of-life-js": "cross-env PROJECT=youtube-game-of-life/only-js vite build --emptyOutDir",
    "build:youtube-css-paint-api": "cross-env PROJECT=youtube-css-paint-api vite build --emptyOutDir",
    "build:ng-test-notes": "cd ./ng-test-notes && npm run build:github",
    "build:readme": "node ./helpers/copyReadme.js",
    "delete-browser-folders": "node ./helpers/delete-browser-folders.js",
    "build": "npm run build:gsap-circle && npm run build:perspective-card && npm run build:security-supplies && npm run build:pointer-events && npm run build:youtube-experience-bar && npm run build:youtube-game-of-life-ts && npm run build:youtube-game-of-life-js && npm run build:angular-tree && npm run build:youtube-css-paint-api && npm run build:ng-test-notes && npm run build:readme && npm run delete-browser-folders"
  },
